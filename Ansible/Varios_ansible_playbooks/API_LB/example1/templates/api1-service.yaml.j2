apiVersion: apps/v1
kind: Deployment
metadata:
  name: api2
spec:
  replicas: 2
  selector:
    matchLabels:
      app: api2
  template:
    metadata:
      labels:
        app: api2
    spec:
      containers:
      - name: api2
        image: myregistry/api2:latest
        ports:
        - containerPort: 80
